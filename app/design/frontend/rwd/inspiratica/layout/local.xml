    <!--
    Default layout, loads most of the pages
    -->

    <layout version="0.1.0">
        <default>
            <reference name="footer">
                <remove name="footer_links"/>
                <remove name="footer_links1"/>
                <remove name="footer_links2"/>
                <remove name="footer_links3"/>


                <block type="cms/block" name="my-account-footer">
                    <action method="setBlockId"><block_id>my-account-footer</block_id></action>
                </block>
                <block type="cms/block" name="our-policies">
                    <action method="setBlockId"><block_id>our-policies</block_id></action>
                </block>

            </reference>

            <reference name="head">
                <block type="core/text" name="nil0bhy">
                    <action method="setText"><text><![CDATA[<script src="//use.typekit.net/nil0bhy.js"></script>]]></text></action>
                </block>
            </reference>
        </default>
    </layout>

